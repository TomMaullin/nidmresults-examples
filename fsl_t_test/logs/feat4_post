</pre><hr>Post-stats<br><pre>

/usr/local/packages/fsl-5.0.7/bin/fslmaths stats/zstat1 -mas mask thresh_zstat1

echo 243478 > thresh_zstat1.vol
zstat1: DLH=0.0474056 VOLUME=243478 RESELS=102.032

/usr/local/packages/fsl-5.0.7/bin/cluster -i thresh_zstat1 -c stats/cope1 -t 3.1 -p 0.05 -d 0.0474056 --volume=243478 --othresh=thresh_zstat1 -o cluster_mask_zstat1 --connectivity=26 --mm --olmax=lmax_zstat1_std.txt --scalarname=Z > cluster_zstat1_std.txt

/usr/local/packages/fsl-5.0.7/bin/cluster2html . cluster_zstat1 -std

/usr/local/packages/fsl-5.0.7/bin/fslstats thresh_zstat1 -l 0.0001 -R 2>/dev/null
